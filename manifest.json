{
  "manifest_version": 3,
  "name": "GitHub Contextual Merge Strategy",
  "version": "2.0.1",
  "description": "Defines a contextual merge strategy on GitHub pull requests according to the source branch.",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "https://github.com/drata/*"
  ]
}
